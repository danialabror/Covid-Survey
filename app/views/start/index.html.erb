   </head>

   <body>

    <main>

        <!-- Slider Area Start-->
        <div class="slider-area ">
            <div class="slider-active">
                <div class="single-slider slider-height slider-padding sky-blue d-flex align-items-center">
                    <div class="container">
                        <div class="row d-flex align-items-center">
                            <div class="col-lg-6 col-md-9 ">
                                <div class="hero__caption">
                                    <span data-animation="fadeInUp" data-delay=".4s">WELCOME</span>
                                    <h1 data-animation="fadeInUp" data-delay=".6s">Coronavirus<br>survey</h1>
                                    <p data-animation="fadeInUp" data-delay=".8s">“Yang terbesar dari kebodohan adalah mengorbankan kesehatan untuk jenis lain dari kebahagiaan.”</p>
                                    <!-- Slider btn -->
                                   <div class="slider-btns">
                                        <!-- Hero-btn -->
                                        <%= link_to  "Start Survey", surveys_index_path, class: 'btn btn-radius' %>
                                        </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="hero__img d-none d-lg-block f-right" data-animation="fadeInRight" data-delay="1s">
                                    <%= image_tag 'hero/hero_right.png' %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
                            
         <%= javascript_tag 'popper.min.js', 'data-turbolinks-track': 'reload' %>
